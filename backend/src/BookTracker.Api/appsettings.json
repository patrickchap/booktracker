{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "PostgreSQL": "Host=localhost;Database=booktracker;Username=postgres;Password=postgres",
    "Redis": "localhost:6379"
  },
  "GoogleBooks": {
    "ApiKey": "YOUR_GOOGLE_BOOKS_API_KEY",
    "BaseUrl": "https://www.googleapis.com/books/v1"
  },
  "Authentication": {
    "Google": {
      "ClientId": "YOUR_GOOGLE_CLIENT_ID.apps.googleusercontent.com",
      "ClientSecret": "YOUR_GOOGLE_CLIENT_SECRET"
    },
    "Jwt": {
      "Secret": "your-super-secret-jwt-key-that-should-be-at-least-32-characters-long",
      "Issuer": "BookTracker",
      "Audience": "BookTracker",
      "ExpirationMinutes": 60
    }
  }
}
